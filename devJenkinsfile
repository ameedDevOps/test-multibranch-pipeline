pipeline {
   agent any
   stages {
       stage('Build Code') {
           steps {
               echo 'Building Artifact from Develop Branch'
               account_id=$(aws sts get-caller-identity --query "Account" --output text)
               echo "$account_id"
           }
       }
      stage('Deploy Code') {
          steps {
          input 'Should we deploy to Production Environment?'
               echo 'Deploying Code from Develop Branch'
          }
      }
   }
}
