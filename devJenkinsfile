pipeline {
   agent any
    environment {
        ACCOUNT_ID = credentials('account_id')
        NAME = credentials('name_id')
    }
   stages {
       stage('Build Code') {
           steps {
               echo 'Building Artifact from Develop Branch'
               echo "${ACCOUNT_ID}"
           }
       }
      stage('Deploy Code') {
          steps {
          input 'Should we deploy to Production Environment?'
               echo 'Deploying Code from Develop Branch'
          }
      }
   }
}
